#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/nuxt-workshop-devcon2024-preparations/nuxt-app/node_modules/.pnpm/openapi-typescript@6.7.6/node_modules/openapi-typescript/bin/node_modules:/workspaces/nuxt-workshop-devcon2024-preparations/nuxt-app/node_modules/.pnpm/openapi-typescript@6.7.6/node_modules/openapi-typescript/node_modules:/workspaces/nuxt-workshop-devcon2024-preparations/nuxt-app/node_modules/.pnpm/openapi-typescript@6.7.6/node_modules:/workspaces/nuxt-workshop-devcon2024-preparations/nuxt-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/nuxt-workshop-devcon2024-preparations/nuxt-app/node_modules/.pnpm/openapi-typescript@6.7.6/node_modules/openapi-typescript/bin/node_modules:/workspaces/nuxt-workshop-devcon2024-preparations/nuxt-app/node_modules/.pnpm/openapi-typescript@6.7.6/node_modules/openapi-typescript/node_modules:/workspaces/nuxt-workshop-devcon2024-preparations/nuxt-app/node_modules/.pnpm/openapi-typescript@6.7.6/node_modules:/workspaces/nuxt-workshop-devcon2024-preparations/nuxt-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
